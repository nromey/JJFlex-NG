<local:JJFlexDialog x:Class="JJFlexWpf.Dialogs.SelectScanDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:JJFlexWpf"
        Title="Select a scan"
        Width="240" Height="220"
        SizeToContent="Height">
    <StackPanel Margin="12">
        <ListBox x:Name="NameListBox" Height="120"
                 AutomationProperties.Name="Saved scans"
                 MouseDoubleClick="NameListBox_MouseDoubleClick" />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,8,0,0">
            <Button Content="OK" MinWidth="80" Height="28" Margin="0,0,8,0"
                    IsDefault="True" Click="OKButton_Click"
                    AutomationProperties.Name="OK" />
            <Button Content="Cancel" MinWidth="80" Height="28"
                    IsCancel="True"
                    AutomationProperties.Name="Cancel" />
        </StackPanel>
    </StackPanel>
</local:JJFlexDialog>
