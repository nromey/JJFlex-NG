<local:JJFlexDialog x:Class="JJFlexWpf.Dialogs.PanListDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="clr-namespace:JJFlexWpf"
        Title="Select Pan Range"
        Width="300" Height="280"
        SizeToContent="Height">
    <StackPanel Margin="12">
        <ListBox x:Name="RangeList" Height="180"
                 AutomationProperties.Name="Pan ranges"
                 MouseDoubleClick="RangeList_DoubleClick"
                 KeyDown="RangeList_KeyDown" />
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,8,0,0">
            <Button Content="Cancel" MinWidth="80" Height="28"
                    IsCancel="True"
                    AutomationProperties.Name="Cancel" />
        </StackPanel>
    </StackPanel>
</local:JJFlexDialog>
